#PBS -S /bin/bash
#PBS -N devel_example_256cores_has
#PBS -l select=11:ncpus=24:model=has
#PBS -q devel
#PBS -l walltime=2:00:00
#PBS -j oe
#PBS -W group_list=s2051
#PBS -m e

module purge
module load mpi-sgi/mpt
module load comp-intel

export MPI_BUFS_PER_PROC=256
export OMP_NUM_THREADS=1

cd $PBS_O_WORKDIR

mpiexec -np 256 ./rayleigh.opt -nprow 16 -npcol 16 >> logfile1
