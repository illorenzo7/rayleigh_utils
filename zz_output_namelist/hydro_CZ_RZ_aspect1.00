equatorial_nrec      = 1

full3d_values = 1,2,3, 301,302,303, 501,502
full3d_frequency = 250000

! Now the average/timetraces, containing many variables
! concerning the dynamics

azavg_values = 
    ! FLUID VARIABLES
    1,2,3, 301,302,303, 501,502, 
    ! PLUS RADIAL/THETA DERIVS of S, P
    507,508, 513,514,
    ! DIAGONAL PARTS OF GRAD V: for an explicit divergence and other tensors
!    10,20, ! dvr/dr, dvtheta/dtheta (dvphi/dphi=0 in AZ_Avgs)
    ! OFF-DIAGONAL PARTS OF GRAD V: if we want rates-of-strain, and we don't
                                    ! don't want to use the vorticity
!    11,12, 19,21, ! dvtdr,dvpdr, dvrdt,dvpdt; ddp terms are all zero in AZ_Avgs 
    ! ENSTROPHY    
    314,315,316, 317,318,319, !vort_{r,t,p}_sq, vortfluc{r,tp}_sq
    ! KINETIC ENERGY
	402,403,404, 410,411,412,
	414,415,416, 422,423,424,
    ! THERMAL ENERGY
    701,704, !internal energy, enthalpy (density)
    ! THERMAL ENERGY EQUATION !, Conductive, Radiative, Viscous)
	1401,1402,1406, ! (minus?) Advection (tot., fluc., ref.)
    1421,1434,1435, ! (vol. heating: conductive, internal, viscous)
    ! Terms for TOTAL ENERGY EQUATION
    1901,1907,1910,1440,
    ! Pressure work, viscous work on KE, minus advec. work, rhot*v_r*S
    ! ENTHALPY FLUX (r,theta,phi: tot, pp)
	1455,1456,1457, 1458,1459,1460, 
    ! CONDUCTIVE FLUX (r,theta,phi=0: tot; fluc=0)
    1470,1471,
    ! VOL. HEAT FLUX
    1433, 
    ! KE FLUX (total: r,theta,phi; mke_mflux; pke_mflux; pke_pflux)
    1923,1924,1925, 1926,1927,1928, 1929,1930,1931, 1932,1933,1934,
    ! VISCOUS FLUX
    1935,1936,1937, 1938,1939,1940, 
    ! TORQUE (AMOM PRODUCTION TERMS): RS, mean advec., Coriolis, viscous
	1801,1802, 1803, 1804, 
    ! AMOM FLUX (r and theta: RS, MC_vphi, MC_om
	1807,1808, 1809,1810, 1811,1812, 1813,1814, 
    ! AMOM DENSITY
    1819,1820,1821, 1822,1823,1824, 1825,1826,1827,
    ! MOMENTUM: 
	1201,1202,1203, 1211,1212, 1214,1215, ! ADV: (r,theta,phi: pp,mm) (Something wrong with radial
                                           ! bit (adv_pp = adv_mm) so keep the total adv_r for now)
    1216, 1219,1220,1221, ! BUOYANCY (r), CORIOLIS (r, theta, phi)
    1228,1229,1230, ! VISCOUS (r, theta, phi)
    1237,1238,1239, ! PRESSURE (r, theta, phi)
    1246, 1247!,  ! ELL0: Buoyancy, Pressure (These exactly balance one another, but shouldn't)
azavg_frequency = 2500
azavg_nrec = 1

